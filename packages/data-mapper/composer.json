{
    "name": "elaitech/data-mapper",
    "description": "Elaitech DataMapper - A reusable data mapping and transformation component for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Elaitech",
            "email": "info@elaitech.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "spatie/laravel-data": "^4.19"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.5.3",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Elaitechx\\DataMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elaitech\\DataMapper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elaitech\\DataMapper\\DataMapperServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
